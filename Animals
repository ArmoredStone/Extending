package extending;

public class Animals extends Extending {
    public int weight;
    public int size;
    public int color;

    public Animals() {
        System.out.println("pystoj constructor Animals");
    }

    public Animals(int ves, int razmer, int okras) {
        System.out.println("constructor parametrami Animals");
        this.weight = ves;
        this.size = razmer;
        this.color = okras;
        this.animalsBirth();
    }

    void mammalsBirth() {
        System.out.println("Mammals rodilsja :) ");
    }
    void fishBirth(){
        System.out.println("Fish rodilsja");
    }

    @Override
    public String toString() {
        System.out.println("toString Animals");
        return "Animals{" + "weight=" + weight + ", size=" + size + ", color=" + color + '}';
    }



}
